struct Storage<ItemType> {
    data:ItemType;

    func set(self:$Storage<ItemType>, val:ItemType) {
        self.data = val;
        return;
    }

    func get(self:$Storage<ItemType>) ~> ItemType {
        return self.data;
    }
}


export func main(num: i32) ~> i32 {
    let store:Storage<i32>;

    store.set(5);
    libc_printf("value: %i\n":$c8, store.get());
    return 0;
}